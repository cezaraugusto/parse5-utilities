{
  "name": "parse5-utilities",
  "description": "Low-level parse5 node manipulation utilities. ESM-compatible version of parse5-utils.",
  "version": "0.0.1",
  "type": "module",
  "packageManager": "pnpm@9.9.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build --watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@rslib/core": "^0.9.1",
    "@types/node": "^22.15.21",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "parse5": "^7.3.0"
  },
  "keywords": [
    "html",
    "parse5",
    "utilities",
    "ast"
  ]
}
